{
  "expo": {
    "name": "HoyaHacks",
    "slug": "HoyaHacks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/baymax_face.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.HoyaHacks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/baymax_face.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.HoyaHacks"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/baymax_face.png",
          "imageResizeMode": "contain",
          "backgroundColor": "#FFFFFF"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "52e6f50e-4e36-4772-8dbc-aa87a9a72afe"
      }
    }
  }
}
